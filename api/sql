create database if not exists dp;
	use dp;
create table if not exists doctor(
	id int auto_increment primary key,
	email varchar(500),
	password varchar(500)
);

create table if not exists patient(
	id int auto_increment primary key,
	name varchar(500),
	email varchar(500),
	password varchar(500),
	phone varchar(10),
	diagnosis varchar(500),
	medical varchar(500),
	address varchar(500),
	country varchar(500),
	state varchar(500),
	city varchar(500),
	pincode varchar(500),
	dt timestamp
);



